<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Project.model.CourseDetails" table="CourseDetails" schema="Project">
        <id name="id" column="id"/>
        <property name="name" column="Name"/>
        <property name="courseId" column="Course_ID"/>
        <property name="courseDescription" column="Course_Description"/>
        <property name="instructorName" column="Instructor_Name"/>
        <property name="time" column="Time"/>
        <property name="day" column="Day"/>
    </class>
</hibernate-mapping>